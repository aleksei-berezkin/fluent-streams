{
  "name": "fluent-streams",
  "version": "0.1.0",
  "description": "Fluent API for JavaScript iterables processing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist ; rm -rf docs ; jest --clearCache",
    "test": "jest",
    "compile": "tsc --project tsconfig.build.json",
    "rm-impl-typedef": "rm -f dist/impl/*.d.ts",
    "doc": "typedoc src --tsconfig tsconfig.build.json --excludeNotDocumented --mode file --theme default",
    "nojekyll": "touch docs/.nojekyll",
    "build": "npm-run-all clean test compile rm-impl-typedef doc nojekyll",
    "bench-compile": "tsc --project tsconfig.bench.json",
    "bench-run": "node dist/bench/index.js",
    "bench": "npm-run-all bench-compile bench-run",
    "serve": "serve -n docs"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksei-berezkin/fluent-streams.git"
  },
  "keywords": [
    "stream",
    "iterable",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksei-berezkin/fluent-streams/issues"
  },
  "homepage": "https://github.com/aleksei-berezkin/fluent-streams#readme",
  "devDependencies": {
    "@types/benchmark": "^1.0.33",
    "@types/jest": "^26.0.5",
    "@types/lazy.js": "^0.5.2",
    "benchmark": "^2.1.4",
    "jest": "^26.1.0",
    "lazy.js": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "sequency": "^0.19.2",
    "serve": "^11.3.2",
    "ts-jest": "^26.1.3",
    "typedoc": "^0.19.1",
    "typescript": "^3.9.7"
  }
}
